%h1 Listing uploads

#uploads
- @uploads.each do |upload|
  = render(upload)

  %span= link_to 'Show', upload
  %span= link_to 'Edit', edit_upload_path(upload)
  %span= link_to 'Destroy', upload, :method => :delete, :data => { :confirm => 'Are you sure?' }

%br

= link_to 'New Upload', new_upload_path, "data-no-turbolink" => true
